{
  "presets": ["react", "es2015", "es2016", "stage-0"],
  "plugins": [
    "transform-decorators-legacy",
    "babel-plugin-transform-object-rest-spread",
    ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": "css" }],
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ]
  ]
}